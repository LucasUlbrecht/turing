<config xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xmlns='http://www.ehcache.org/v3' xsi:schemaLocation="
            http://www.ehcache.org/v3 
            http://www.ehcache.org/schema/ehcache-core-3.7.xsd">
    <persistence directory="store/cache" />
    <cache-template name="default">
        <expiry>
            <ttl unit="seconds">30</ttl>
        </expiry>
        <resources>
            <heap>1000</heap>
            <offheap unit="MB">10</offheap>
            <disk persistent="true" unit="MB">20</disk>
        </resources>
    </cache-template>

    <cache alias="solrCore" uses-template="default"></cache>
    <cache alias="spotlight" uses-template="default"></cache>
    <cache alias="turSEInstancefindAll" uses-template="default"></cache>
    <cache alias="turSEVendorfindAll" uses-template="default"></cache>
    <cache alias="turSEInstancefindById" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSite" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndFacetAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndHlAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndMltAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndRequiredAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldExtfindByTurSNSiteAndNlpAndEnabled" uses-template="default"></cache>
    <cache alias="turSNSiteFieldfindById" uses-template="default"></cache>
    <cache alias="turSNSiteFieldfindByTurSNSite" uses-template="default"></cache>
    <cache alias="turSNSitefindByName" uses-template="default"></cache>
    <cache alias="turConfigVarfindAll" uses-template="default"></cache>
    <cache alias="turConfigVarfindById" uses-template="default"></cache>
    <cache alias="turLocalefindAll" uses-template="default"></cache>
    <cache alias="turLocalefindByInitials" uses-template="default"></cache>
    <cache alias="nlpName" uses-template="default"></cache>
    <cache alias="nlpSentence" uses-template="default"></cache>
    <cache alias="nlpToken" uses-template="default"></cache>
</config>